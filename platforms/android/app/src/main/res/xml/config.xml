<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mdc.pidoyaConductor" version="0.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <allow-navigation href="cdvfile:*" />
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="InAppBrowser">
        <param name="android-package" value="org.apache.cordova.inappbrowser.InAppBrowser" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FirebaseAnalytics">
        <param name="android-package" value="by.chemerisuk.cordova.firebase.FirebaseAnalyticsPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FirebaseMessaging">
        <param name="android-package" value="by.chemerisuk.cordova.firebase.FirebaseMessagingPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Pidoya Conductor</name>
    <description>
        Nuestra misi√≥n es facilitarte la vida
    </description>
    <author email="desarrollo@mdccolombia.com" href="http://phonegap.com">
        MDC COLOMBIA
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <icon density="ldpi" src="www/res/android/icon/drawable-ldpi-icon.png" />
    <icon density="mdpi" src="www/res/android/icon/drawable-mdpi-icon.png" />
    <icon density="hdpi" src="www/res/android/icon/drawable-hdpi-icon.png" />
    <icon density="xhdpi" src="www/res/android/icon/drawable-xhdpi-icon.png" />
    <icon density="xxhdpi" src="www/res/android/icon/drawable-xxhdpi-icon.png" />
    <icon density="xxxhdpi" src="www/res/android/icon/drawable-xxxhdpi-icon.png" />
    <splash density="land-ldpi" src="www/res/android/splash/drawable-land-ldpi-screen.png" />
    <splash density="land-mdpi" src="www/res/android/splash/drawable-land-mdpi-screen.png" />
    <splash density="land-hdpi" src="www/res/android/splash/drawable-land-hdpi-screen.png" />
    <splash density="land-xhdpi" src="www/res/android/splash/drawable-land-xhdpi-screen.png" />
    <splash density="land-xxhdpi" src="www/res/android/splash/drawable-land-xxhdpi-screen.png" />
    <splash density="land-xxxhdpi" src="www/res/android/splash/drawable-land-xxxhdpi-screen.png" />
    <splash density="port-ldpi" src="www/res/android/splash/drawable-port-ldpi-screen.png" />
    <splash density="port-mdpi" src="www/res/android/splash/drawable-port-mdpi-screen.png" />
    <splash density="port-hdpi" src="www/res/android/splash/drawable-port-hdpi-screen.png" />
    <splash density="port-xhdpi" src="www/res/android/splash/drawable-port-xhdpi-screen.png" />
    <splash density="port-xxhdpi" src="www/res/android/splash/drawable-port-xxhdpi-screen.png" />
    <splash density="port-xxxhdpi" src="www/res/android/splash/drawable-port-xxxhdpi-screen.png" />
    <allow-intent href="market:*" />
    <resource-file src="google-services.json" target="app/google-services.json" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="GradlePluginGoogleServicesEnabled" value="true" />
    <preference name="GradlePluginGoogleServicesVersion" value="4.3.3" />
    <preference name="AndroidXEnabled" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-targetSdkVersion" value="29" />
    <preference name="phonegap-version" value="cli-7.1.0" />
    <preference name="Orientation" value="portrait" />
    <preference name="BackupWebStorage" value="none" />
</widget>
