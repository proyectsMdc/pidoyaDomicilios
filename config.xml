<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mdc.pidoyaConductor" version="0.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Pidoya Conductor</name>
    <description>
        Nuestra misión es facilitarte la vida
    </description>
    <author email="desarrollo@mdccolombia.com" href="http://phonegap.com">
